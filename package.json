{
  "name": "myinspire-ph.ru-v4",
  "version": "4.0.0",
  "description": "The 4rd version of the site [React + TypeScript + Next]",
  "scripts": {
    "start": "next start",
    "build": "webpack -c webpack.pre-build.config.js && next build",
    "build:only": "next build",
    "dev": "next dev",
    "test": "eslint --ext .ts,.tsx ./ && stylelint ./**/*.scss",
    "fix": "eslint --ext .ts,.tsx --fix ./ && stylelint --fix ./**/*.scss"
  },
  "author": "Alexandrovich Dmitriy <yoskutik@gmail.com>",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm --no-git-tag-version version patch && git add package.json"
    }
  },
  "lint-staged": {
    "*.(ts|tsx)": [
      "eslint --fix",
      "git add"
    ],
    "*.scss": [
      "stylelint --fix",
      "git add"
    ]
  },
  "browserslist": [
    "defaults",
    "Safari > 6",
    "iOS 6",
    "not IE 11"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.7.0",
    "@typescript-eslint/parser": "5.7.0",
    "eslint": "8.5.0",
    "eslint-config-airbnb-typescript": "16.1.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.27.1",
    "eslint-plugin-react-hooks": "4.3.0",
    "husky": "7.0.4",
    "lint-staged": "12.1.3",
    "sass": "1.45.0",
    "webpack-dev-server": "4.6.0"
  },
  "dependencies": {
    "@types/node-telegram-bot-api": "0.53.2",
    "@types/react": "17.0.37",
    "cli-progress": "3.9.1",
    "colors": "1.4.0",
    "favicons": "6.2.2",
    "favicons-webpack-plugin": "5.0.2",
    "glob": "7.2.0",
    "image-size": "1.0.0",
    "imagemin": "7.0.1",
    "imagemin-webp": "6.0.0",
    "natural": "4.0.4",
    "next": "12.0.7",
    "next-plugin-preact": "3.0.6",
    "node-telegram-bot-api": "0.56.0",
    "path": "0.12.7",
    "preact": "10.6.4",
    "preact-render-to-string": "5.1.19",
    "react": "npm:@preact/compat",
    "react-dom": "npm:@preact/compat",
    "react-ssr-prepass": "npm:preact-ssr-prepass",
    "scheduler": "^0.20.2",
    "sitemap-webpack-plugin": "1.1.1",
    "tsyringe": "^4.6.0",
    "typescript": "4.5.4",
    "webpack": "5.65.0",
    "webpack-cli": "4.9.1"
  },
  "eslintIgnore": [
    "node_modules"
  ]
}
